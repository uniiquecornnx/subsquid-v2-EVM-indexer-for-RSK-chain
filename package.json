{
  "name": "hello-rsk-squid",
  "version": "0.1.0",
  "private": true,
  "scripts": {
  "codegen": "squid-typeorm-codegen",
  "mig:gen": "squid-typeorm-migration generate",
  "mig:apply": "squid-typeorm-migration apply",
  "build": "tsc",
  "process": "node -r dotenv/config lib/processor.js",
  "serve": "squid-graphql-server",
  "db:up": "docker compose up -d",
  "db:down": "docker compose down -v"
},
  "dependencies": {
    "@subsquid/evm-processor": "^1.27.3",
    "@subsquid/graphql-server": "^4.11.0",
    "@subsquid/typeorm-store": "^1.5.1",
    "ethers": "^6.10.0"
  },
  "devDependencies": {
    "@subsquid/cli": "^2.5.0",
    "@subsquid/typeorm-codegen": "^2.0.2",
    "@subsquid/typeorm-migration": "^1.3.0",
    "typescript": "^5.9.3"
  }
}
